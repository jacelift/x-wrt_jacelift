/dts-v1/;

#include "mt7621.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	model = "XWRT MAC500F";
	compatible = "xwrt,mac500f", "mediatek,mt7621-soc";

	aliases {
		led-status = &led_sys;
	};

	chosen {
		bootargs = "console=ttyS0,57600";
	};

	leds {
		compatible = "gpio-leds";

		led_sys: sys {
			label = "xwrt:sys";
			gpios = <&gpio0 10 GPIO_ACTIVE_LOW>;
		};
		sfp {
			label = "xwrt:sfp";
			gpios = <&gpio0 12 GPIO_ACTIVE_LOW>;
		};
	};

	keys {
		compatible = "gpio-keys-polled";
		poll-interval = <20>;

		reset {
			label = "reset";
			gpios = <&gpio0 18 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};
};

&gdma {
	status = "okay";
};

&hsdma {
	status = "okay";
};

&ethernet {
	status = "okay";
	mtd-mac-address = <&factory 0x22>;

	port@5 {
		compatible = "mediatek,mt7621-gsw-port", "mediatek,eth-port";
		/* port idx 5 */
		reg = <5>;

		status = "okay";
		phy-mode = "rgmii";
		phy-handle = <&phy5>;
	};

	mdio-bus {
		status = "okay";

		//reset-gpios = <&gpio0 7 GPIO_ACTIVE_LOW>;

		phy5: ethernet-phy@5 {
			/* mdio addr 7 */
			reg = <7>;
			phy-mode = "rgmii";
		};
	};
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			ralink,group = "uart2", "uart3", "wdt";
			ralink,function = "gpio";
		};
	};
};

&spi0 {
	status = "okay";

	m25p80@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0 0x30000>;
				read-only;
			};

			partition@30000 {
				label = "u-boot-env";
				reg = <0x30000 0x10000>;
				read-only;
			};

			factory: partition@40000 {
				label = "factory";
				reg = <0x40000 0x10000>;
				read-only;
			};

			partition@a0000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x50000 0xfb0000>;
			};
		};
	};
};
